@article{campbell_complex_2013,
	title = {Complex social contagion makes networks more vulnerable to disease outbreaks},
	volume = {3},
	issn = {2045-2322},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3664906&tool=pmcentrez&rendertype=abstract},
	doi = {10.1038/srep01905},
	abstract = {Social network analysis is now widely used to investigate the dynamics of infectious disease spread. Vaccination dramatically disrupts disease transmission on a contact network, and indeed, high vaccination rates can potentially halt disease transmission altogether. Here, we build on mounting evidence that health behaviors - such as vaccination, and refusal thereof - can spread across social networks through a process of complex contagion that requires social reinforcement. Using network simulations that model health behavior and infectious disease spread, we find that under otherwise identical conditions, the process by which the health behavior spreads has a very strong effect on disease outbreak dynamics. This dynamic variability results from differences in the topology within susceptible communities that arise during the health behavior spreading process, which in turn depends on the topology of the overall social network. Our findings point to the importance of health behavior spread in predicting and controlling disease outbreaks.},
	journal = {Scientific Reports},
	author = {Campbell, Ellsworth and Salathé, Marcel},
	year = {2013},
	pmid = {23712758},
	keywords = {Biological, Communicable Diseases, Communicable Diseases: epidemiology, Communicable Diseases: transmission, Computer Simulation, Contact Tracing, Disease Outbreaks, Disease Outbreaks: prevention \& control, Health Behavior, Humans, Models, Social Behavior, Theoretical, Vaccination},
	pages = {1905}
}

