@article{mackellar_young_1996,
	title = {The {Young} {Men}'s {Survey}: methods for estimating {HIV} seroprevalence and risk factors among young men who have sex with men.},
	volume = {111 Suppl},
	issn = {0033-3549},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1382056&tool=pmcentrez&rendertype=abstract},
	abstract = {Traditional sampling methods are unsuitable for determining the levels of human immunodeficiency virus type I infection and related behavioral risk factors among young men who have sex with men. Most surveys of this hard-to-reach population have used nonprobability samples of young men in clinical or public settings. While these studies have revealed high rates of HIV infection and risk behaviors, their findings are not generalizable to broader populations of young men who have sex with men. To better understand the epidemiology of HIV within this population, the Centers for Disease Control and Prevention, in collaboration with state and local health departments, has developed a venue-based probability survey of young men who have sex with men. Conducted in seven metropolitan areas in the United States, the Young Men's Survey combines outreach techniques with standard methods of sample surveys to enumerate, sample, and estimate prevalence outcomes of a population of young men who frequent public venues and who have sex with other men. Venues where young men who have sex with men are sampled include dance clubs, bars, and street locations. At sampled venues, young men are enumerated, consecutively approached, and offered enrollment if they are determined eligible. Young men who agree to participate in the Young Men's Survey are interviewed, counseled, and tested for human immunodeficiency virus, hepatitis B, and syphilis in vans parked near sampled venues. The Young Men's Survey provides data on the locations and times at which demographic and behavioral subgroups of young men who have sex with men may be targeted for prevention activities. Behaviors and psychosocial factors associated with human immunodeficiency virus infection can be used to design culturally relevant and age-specific prevention activities for young men who have sex with men.},
	journal = {Public Health Rep.},
	author = {MacKellar, D and Valleroy, L and Karon, J and Lemp, G and Janssen, R},
	year = {1996},
	pmid = {8862170},
	keywords = {Adolescent, Adult, Data Collection, Data Collection: methods, HIV Seroprevalence, Homosexuality, Humans, Male, Probability, Questionnaires, Risk Factors, Sampling Studies, Sexual Behavior, Sexual Behavior: statistics \& numerical data, United States},
	pages = {138--44},
	file = {Attachment:/Users/g/Documents/zotero_papers/storage/H7H622BH/MacKellar et al. - 1996 - The Young Men's Survey methods for estimating HIV seroprevalence and risk factors among young men who have (2).pdf:application/pdf}
