}

@article{rohe_network_2015,
	title = {Network driven sampling; a critical threshold for design effects},
	url = {http://arxiv.org/abs/1505.05461},
	abstract = {Respondent-driven sampling (RDS) and web crawling are two types of network driven sampling techniques. To study the variance of statistical estimators constructed from these random samples, this paper models the network driven sampling process as a Markov process on the network that is indexed by a tree. Each node in this tree corresponds to an observation and each edge in the tree corresponds to a referral. Indexing with a tree, instead of a chain, allows for the sampled units to refer multiple future units into the sample. Under certain assumptions, Theorem 2.1 gives a closed form expression for the variance of the popular Volz-Heckathorn estimator (VH) as a function of the spectral structure of the Markov transition matrix and a function \${\textbackslash}backslashmathbb\{G\}\$ that is easy to compute and characterizes the referral tree's "bushiness." Under various assumptions on the referral tree, the design effect of network driven sampling has a critical threshold that is a function of the referral rate \$m\$ and the clustering structure in the social network, represented by the second eigenvalue of the Markov transition matrix \${\textbackslash}backslashlambda\_2\$. In the RDS lexicon, a large value of \${\textbackslash}backslashlambda\_2\$ corresponds to a "bottleneck" in the referral process. Under the proposed model, if \$m {\textbackslash}backslashle 1/{\textbackslash}backslashlambda\_2{\textasciicircum}2\$, then the design effect is finite and the VH estimator is \${\textbackslash}backslashsqrt\{n\}\$-consistent. However, if \$m {\textbackslash}textgreater 1/{\textbackslash}backslashlambda\_2{\textasciicircum}2\$, then the design effect grows with \$n\$ and the VH estimator is no longer \${\textbackslash}backslashsqrt\{n\}\$-consistent; it converges at the slower rate of \${\textbackslash}backslashlog\_m {\textbackslash}backslashlambda\_2\$.},
	author = {Rohe, Karl},
	year = {2015},
	pages = {1--27},
	file = {Attachment:/Users/g/Documents/zotero_papers/storage/IQN84QKM/Rohe - 2015 - Network driven sampling a critical threshold for design effects(2).pdf:application/pdf}
}

